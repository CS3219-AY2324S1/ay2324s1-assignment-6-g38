  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "install:services": "npm-run-all --parallel install:frontend install:collab-service install:matching-service install:question-service install:user-service install:history-service",
    "install:frontend": "cd services/frontend && npm install",
    "install:collab-service": "cd services/collab-service && npm install",
    "install:matching-service": "cd services/matching-service-rabbitmq && npm install",
    "install:question-service": "cd services/question-service && npm install",
    "install:user-service": "cd services/user-service && npm install",
    "install:history-service": "cd services/history-service && npm install",
    "start:dev": "docker-compose -f docker-compose.dev.yml up --build -d",
    "start:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "stop:prod": "docker-compose -f docker-compose.prod.yml down",
    "logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "logs:prod": "docker-compose -f docker-compose.prod.yml logs -f",
    "cleanup:dev": "docker-compose -f docker-compose.dev.yml down && docker volume prune -f",
    "cleanup:prod": "docker-compose -f docker-compose.prod.yml down && docker volume prune -f",
    "restart:dev": "npm run stop:dev && npm run start:dev",
    "restart:prod": "npm run stop:prod && npm run start:prod"
  },
